{
  "_version": "1.48.0",
  "sap.app": {
    "id": "project2",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.8.3",
      "toolsId": "3a90360b-f90e-484c-9dce-da391f1a988f"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "mainService2": {
        "uri": "/customer/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project2.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None"

        }
      },
      "Customer": {
        "dataSource": "mainService2",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "earlyRequests":true,
          "synchronizationMode": "None",
          "autoExpandSelect": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",   
        "async": true,
        "viewPath": "project2.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "customer_home",
          "pattern": "",
          "target": [
            "customer_home"
          ],
          "layout" : "OneColumn" 
        },
        {
          "name": "Customer",
          "pattern": "Customer",
          "target": [
            "Customer"
          ],
          "layout" : "OneColumn" 
        },
        {
          "name": "customer_detail",
          "pattern": "customer_detail/{num}",
          "target": [
            "Customer",
            "customer_detail" 
          ],
          "layout": "TwoColumnsMidExpanded"
        },
        {
          "name": "customer_detailexpand",
          "pattern": "customer_detail/{num}/expand",
          "target": [           
            "customer_detail"             
          ],
          "layout": "MidColumnFullScreen"
        },
        {
          "name": "CreateOrganization",
          "pattern": "CreateOrganization",
          "target": [
            "CreateOrganization"
          ],
          "layout": "OneColumn"
        },
        {
          "name": "Customer_chart",
          "pattern": "Customer_chart",
          "target": [
            "Customer_chart"
          ]
        },
        {
          "name": "CreateCustomer",
          "pattern": "CreateCustomer",
          "target": [
            "CreateCustomer"
          ],
          "layout": "OneColumn"
        },
        {
          "name": "DetailOrganization",
          "pattern": "DetailOrganization/{num}",
          "target": [
            "Customer",
            "DetailOrganization"
          ],
          "layout": "TwoColumnsMidExpanded"
        },
        {
          "name": "DetailOrganizationexpand",
          "pattern": "DetailOrganization/{num}/expand",
          "target": [
            "DetailOrganization"
          ],
          "layout": "MidColumnFullScreen"
        }
      ],
      "targets": {
        "customer_home": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "customer_home",
          "viewName": "customer_home",
          "clearControlAggregation": false,
          "controlAggregation": "beginColumnPages"
        },
        "Customer": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "Customer",
          "viewName": "Customer",
          "clearControlAggregation": false,
          "controlAggregation": "beginColumnPages"
        },
        "CreateCustomer": {

          "viewType": "XML",
          "transition": "slide",
          "viewId": "CreateCustomer",
          "viewName": "CreateCustomer",
          "clearControlAggregation": false,
          "controlAggregation": "beginColumnPages"
        },

        "customer_detail": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "customer_detail",
          "viewName": "customer_detail",
          "clearControlAggregation": false,
          "controlAggregation": "midColumnPages"
        },

        "Customer_chart": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "Customer_chart",
          "viewName": "Customer_chart",
          "clearControlAggregation": false,
          "controlAggregation": "beginColumnPages"
        },
        "CreateOrganization": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "CreateOrganization",
          "viewName": "CreateOrganization",
          "clearControlAggregation": false,
          "controlAggregation": "beginColumnPages"
        },

        "DetailOrganization": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "DetailOrganization",
          "viewName": "DetailOrganization",
          "clearControlAggregation": false,
          "controlAggregation": "midColumnPages"
        }
 

      }
    },
    "rootView": {
      "viewName": "project2.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
